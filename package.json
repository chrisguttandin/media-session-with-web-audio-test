{
    "author": "Christoph Guttandin",
    "bugs": {
        "url": "https://github.com/chrisguttandin/media-session-with-web-audio-test/issues"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "description": "A test page for using the Media Session API in combination with the Web Audio API.",
    "devDependencies": {
        "@babel/core": "^7.28.5",
        "@babel/preset-env": "^7.28.5",
        "@commitlint/cli": "^20.3.1",
        "@commitlint/config-angular": "^20.3.0",
        "babel-loader": "^10.0.0",
        "commitizen": "^4.3.1",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^8.57.0",
        "eslint-config-holy-grail": "^61.0.3",
        "html-minifier": "^4.0.0",
        "htmlhint": "^1.8.0",
        "husky": "^9.1.7",
        "hyperlink": "^5.0.4",
        "lint-staged": "^16.2.7",
        "prettier": "^3.7.4",
        "rimraf": "^6.1.2",
        "webpack": "^5.104.1",
        "webpack-cli": "^6.0.1",
        "webpack-dev-server": "^5.2.2"
    },
    "homepage": "https://chrisguttandin.github.io/media-session-with-web-audio-test",
    "license": "MIT",
    "main": "src/index.html",
    "name": "media-session-with-web-audio-test",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/chrisguttandin/media-session-with-web-audio-test.git"
    },
    "scripts": {
        "build": "rimraf build/* && html-minifier --config-file config/html-minifier/config.json --file-ext html --input-dir src --output-dir build && npx webpack --config config/webpack/production.js",
        "lint": "npm run lint:config && npm run lint:src",
        "lint:config": "eslint --config config/eslint/config.json --ext .cjs --ext .js --ext .mjs --report-unused-disable-directives config/",
        "lint:src": "eslint --config config/eslint/src.json --ext .js --report-unused-disable-directives src/ && htmlhint --config config/htmlhint/document.json 'src/**/index.html'",
        "monitor": "npx webpack server --config config/webpack/development.js",
        "prepare": "husky",
        "test": "npm run lint"
    },
    "version": "1.0.0"
}
